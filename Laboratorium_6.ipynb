    "def sumacyfr (value) \n",
    "\n",
    "liczba = value.to_s\n",
    "liczba = liczba.split(\"\").to_a\n",
    "liczba = liczba.map{|e| e.to_i}\n",
    "liczba.inject{|suma, elem| suma + elem}\n",
    "\n",
    "end\n",
    "\n",
    "liczby = (0..100).to_a\n",
    "liczby = liczby.select{|e| e % 2 == 0}\n",
    "liczby = liczby.reject{|e| e % 4 == 0}\n",
    "\n",
    "for liczba in liczby do\n",
    "  \n",
    "  sumacyfr(liczba)\n",
    "  if sumacyfr(liczba) % 7 == 0\n",
    "  puts liczba\n",
    "  end\n",
    "\n",
    "end"
